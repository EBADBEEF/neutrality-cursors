<theme name="mootral">
  <source size="24" gridsize="32" margin="0" spacing="1">
    <image file="build/frames/Hotspots.png" use="hotspot"/>
    <image file="build/frames/Frame0.png" use="0"/>
    <image file="build/frames/Frame1.png" use="1"/>
    <image file="build/frames/Frame2.png" use="2"/>
    <image file="build/frames/Frame3.png" use="3"/>
    <image file="build/frames/Frame4.png" use="4"/>
    <image file="build/frames/Frame5.png" use="5"/>
    <image file="build/frames/Frame6.png" use="6"/>
    <image file="build/frames/Frame7.png" use="7"/>
  </source>
  <layout>
    <row>
      <cursor name="alias" />
      <cursor name="cell" />
      <cursor name="copy" />
      <cursor name="crosshair" />
      <cursor name="default" />
      <cursor name="e-resize" />
      <cursor name="ew-resize" />
      <cursor name="help" />
    </row>
    <row>
      <cursor name="grab" />
      <cursor name="ne-resize" />
      <cursor name="nesw-resize" />
      <cursor name="no-drop" />
      <cursor name="not-allowed" />
      <cursor name="n-resize" />
      <cursor name="ns-resize" />
      <cursor name="nw-resize" />
    </row>
    <row>
      <cursor name="nwse-resize" />
      <cursor name="pointer" />
      <cursor name="progress">
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
      </cursor>
      <cursor name="se-resize" />
      <cursor name="s-resize" />
      <cursor name="sw-resize" />
      <cursor name="text" />
      <cursor name="up-arrow" />
    </row>
    <row>
      <cursor name="wait">
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
        <frame delay="125"/>
      </cursor>
      <cursor name="w-resize" />
      <cursor name="vertical-text" />
      <cursor name="zoom-in" />
      <cursor name="zoom-out" />
      <cursor name="all-scroll" />
      <cursor name="dnd-ask" />
      <cursor name="dnd-copy" />
    </row>
    <row>
      <cursor name="dnd-link" />
      <cursor name="dnd-move" />
      <cursor name="dnd-none" />
      <cursor name="pencil" />
      <cursor name="grabbing" />
      <cursor name="move" />
      <cursor name="context-menu" />
      <cursor name="X_cursor" />
    </row>
  </layout>

